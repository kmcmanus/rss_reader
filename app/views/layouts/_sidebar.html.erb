<% if user_signed_in? %>

<a data-target="#subscription-modal" role="button" class="btn" data-toggle="modal">Subscribe</a>
<ul>
  <% current_user.feeds.each do |feed| %>
    <li><%= link_to feed.name, feed_path(feed) %></li>
  <% end %>
</ul>
<%= render :partial => "shared/subscribe_modal" %>
<% end %>
